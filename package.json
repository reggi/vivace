{
  "name": "vivace",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node server",
    "dev": "./scripts/dev.sh",
    "test": "npm run test-client",
    "test-client": "node ./node_modules/karma/bin/karma start test/karma-dev.conf.js --single-run --browsers PhantomJS",
    "test-client-ci": "node ./node_modules/karma/bin/karma start test/karma-ci.conf.js --single-run --browsers PhantomJS",
    "test-watch": "node ./node_modules/karma/bin/karma start test/karma-dev.conf.js --browsers PhantomJS",
    "test-ci": "npm run test-server && npm run test-client-ci",
    "test-server": "NODE_ENV=test DISABLE_SQL_LOGGING=true ./node_modules/.bin/mocha -r ./test/mocha_helpers.js -R spec --recursive test/server",
    "coverage-push": "cat $(pwd)/test/coverage/lcov.info | ./node_modules/.bin/coveralls",
    "db": "npm run db:sync && npm run db:populate",
    "db:sync": "node ./dev/syncdb",
    "db:populate": "node ./dev/populatedb",
    "build": "webpack -p",
    "postinstall": "node ./scripts/postinstall"
  },
